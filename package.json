{
  "name": "shrey-gitsite",
  "version": "1.0.0",
  "description": "my site on github",
  "main": "index.js",
  "scripts": {
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o styles styles",
    "watch:scss": "onchange \"styles/*.scss\" -- npm run scss",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\"",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShreyNiraula/ShreyNiraula.github.io.git"
  },
  "author": "Shrey Niraula",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ShreyNiraula/ShreyNiraula.github.io/issues"
  },
  "homepage": "https://github.com/ShreyNiraula/ShreyNiraula.github.io#readme",
  "dependencies": {
    "bootstrap": "^4.0.0",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.12.9"
  },
  "devDependencies": {
    "lite-server": "^2.5.4",
    "node-sass": "^4.14.1",
    "onchange": "^3.3.0",
    "parallelshell": "^3.0.1",
    "rimraf": "^2.6.2"
  }
}
